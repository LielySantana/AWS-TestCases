{
    "compilerOptions": {
        "target": "esnext",
        "lib": ["es2020"],
        "strict": true,
        "noEmitOnError": true,
        "sourceMap": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
